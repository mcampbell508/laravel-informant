{
    "name": "mcampbell508/laravel-informant",
    "description": "Run checks against Laravel application dependencies to provide feedback during development.",
    "keywords": ["skeleton"],
    "homepage": "https://github.com/mcampbell508/laravel-informant",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Campbell",
            "email": "matthewcampbell508@gmail.com",
            "homepage": "http://mcampbell508.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/process": "^3.4",
        "symfony/console": "^3.4",
        "symfony/finder": "^3.4",
        "league/climate": "~3.2"
    },
    "require-dev": {
        "mockery/mockery": "dev-master@dev",
        "phpunit/phpunit": "^6.5",
        "friendsofphp/php-cs-fixer": "2.10",
        "orchestra/testbench": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "MattCampbell\\Laravel\\Informant": ["src"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MattCampbell\\Tests\\": ["tests"]
        }
    }
}
